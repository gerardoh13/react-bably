{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/example.json",
  "type": "object",
  "properties": {
    "method": {
      "type": "string",
      "enum": ["bottle", "nursing"]
    },
    "fed_at": {
      "type": "integer"
    },
    "amount": {
      "type": "number"
    },
    "duration": {
      "type": "integer"
    }
  },
  "additionalProperties": false,
  "required": ["method", "fed_at"]
}

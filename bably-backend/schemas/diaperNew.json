{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/example.json",
  "type": "object",
  "properties": {
    "type": {
      "type": "string",
      "enum": ["dry", "wet", "soiled", "mixed"]
    },
    "changed_at": {
      "type": "integer"
    },
    "size": {
      "type": "string",
      "enum": ["light", "medium", "heavy"]

    },
    "infant_id": {
      "type": "integer"
    }
  },
  "additionalProperties": false,
  "required": ["type", "size", "changed_at", "infant_id"]
}

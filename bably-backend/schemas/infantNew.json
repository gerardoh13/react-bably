{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/example.json",
    "type": "object",
    "properties": {
      "firstName": {
        "type": "string",
        "minLength": 2,
        "maxLength": 15
      },
      "dob": {
        "type": "string",
        "format": "date"
      },
      "gender": {
        "type": "string",
        "enum": ["male", "female"]
      },
      "publicId": {
        "type": "string"
      }
    },
    "additionalProperties": false,
    "required": [
      "firstName",
      "dob",
      "gender"
    ]
  }